import qrcode

#img = qrcode.make('http://nftydaze.com')

#print(type(img))
#print(img.size)
# <class 'qrcode.image.pil.PilImage'>
# (290, 290)

#img.save('qrcode_test.png')


qr = qrcode.QRCode()

qr.add_data('http://nftydaze.com')
qr.make()
img = qr.make_image(fill_color="black", back_color="#FAF9F6")
img.save('qrcode_test.png')
